<mxfile host="app.diagrams.net">
  <diagram name="Receiver Pipeline">
    <mxGraphModel dx="1000" dy="800" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- Root -->
        <mxCell id="root" value="Receiver System" style="rounded=1;fillColor=#ffe599;fontSize=16;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="200" y="50" width="400" height="600" as="geometry"/>
        </mxCell>

        <!-- Parent: Receiver Class -->
        <mxCell id="receiver_class" value="Receiver Class" style="rounded=1;fillColor=#c9daf8;fontSize=14;fontStyle=1;" vertex="1" parent="root">
          <mxGeometry x="50" y="50" width="300" height="400" as="geometry"/>
        </mxCell>

        <!-- Children of Receiver Class -->
        <mxCell id="aruco_sync" value="ArUco Sync Stage" style="rounded=1;fillColor=#d9ead3;" vertex="1" parent="receiver_class">
          <mxGeometry x="20" y="20" width="260" height="60" as="geometry"/>
        </mxCell>

        <mxCell id="color_calibration" value="Color Calibration Stage" style="rounded=1;fillColor=#f4cccc;" vertex="1" parent="receiver_class">
          <mxGeometry x="20" y="100" width="260" height="60" as="geometry"/>
        </mxCell>

        <mxCell id="syncing" value="Syncing Stage" style="rounded=1;fillColor=#fff2cc;" vertex="1" parent="receiver_class">
          <mxGeometry x="20" y="180" width="260" height="60" as="geometry"/>
        </mxCell>

        <mxCell id="bit_decoding" value="Bit Decoding Stage" style="rounded=1;fillColor=#d9d2e9;" vertex="1" parent="receiver_class">
          <mxGeometry x="20" y="260" width="260" height="60" as="geometry"/>
        </mxCell>

        <mxCell id="default_stage" value="Default / Fallback Stage" style="rounded=1;fillColor=#fce5cd;" vertex="1" parent="receiver_class">
          <mxGeometry x="20" y="340" width="260" height="40" as="geometry"/>
        </mxCell>

        <!-- Parent: Threads Class -->
        <mxCell id="threads_class" value="Threads Class" style="rounded=1;fillColor=#d0e0e3;fontSize=14;fontStyle=1;" vertex="1" parent="root">
          <mxGeometry x="50" y="470" width="300" height="100" as="geometry"/>
        </mxCell>

        <!-- Children of Threads Class -->
        <mxCell id="decoding_thread" value="Decoding Worker Thread" style="rounded=1;fillColor=#a4c2f4;" vertex="1" parent="threads_class">
          <mxGeometry x="20" y="10" width="260" height="35" as="geometry"/>
        </mxCell>

        <mxCell id="watchdog_thread" value="Watchdog Thread" style="rounded=1;fillColor=#b6d7a8;" vertex="1" parent="threads_class">
          <mxGeometry x="20" y="55" width="260" height="35" as="geometry"/>
        </mxCell>

        <!-- Arrows / Flow -->
        <mxCell id="arrow1" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#000000;endArrow=classic;" edge="1" parent="root" source="aruco_sync" target="color_calibration">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="arrow2" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#000000;endArrow=classic;" edge="1" parent="root" source="color_calibration" target="syncing">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="arrow3" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#000000;endArrow=classic;" edge="1" parent="root" source="syncing" target="bit_decoding">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="arrow4" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#000000;endArrow=classic;" edge="1" parent="root" source="bit_decoding" target="threads_class">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
